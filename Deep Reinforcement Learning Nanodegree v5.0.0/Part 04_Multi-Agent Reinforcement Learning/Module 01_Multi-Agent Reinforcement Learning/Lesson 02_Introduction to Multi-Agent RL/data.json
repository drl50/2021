        {
          "id": 752052,
          "key": "00000510-9f82-4e1c-a069-bff95755a4bf",
          "title": "Lab Instructions",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "00000510-9f82-4e1c-a069-bff95755a4bf",
            "completed_at": null,
            "last_viewed_at": null,
            "unstructured": null
